#include<stdio.h>
#include<stdlib.h>

   main() {
   	 int marks;
   	
   	 printf("enter your marks:");
   	 scanf("%d/n ,&marks");
   	 
    if(marks>=85)
    printf("GRADE A");
		  	 
    else if(marks>=70)
    printf("GRADE B");
    
    else if(marks>=55)
    printf("GRADE C");
      
    else if(marks>=40)
    printf("GRADE D");
        
    else if(marks<40)
    printf("GRADE F");
   }
